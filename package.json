{
  "name": "super-server-api-node",
  "version": "0.1.0",
  "description": "Super server RESTful API with Node.js Express",
  "main": ".",
  "private": true,
  "author": "M Haidar Hanif <npm@mhaidarhanif.com>",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon -i '*.test.js' .",
    "test": "cross-env NODE_ENV=test nyc ava",
    "test:unit": "npm test './**/!(*.router*).test.js'",
    "test:integration": "npm test './**/*.router.test.js'",
    "coverage": "nyc report -r lcov && opn coverage/lcov-report/index.html",
    "prod": "npm run build && nodemon -i '*.test.js' -r dotenv-safe/config dist",
    "lint": "eslint './**/*.js'",
    "clean": "del 'dist/!(.git*|Procfile)**'",
    "copy": "copy package.json dist && copy ./**/package.json dist/.",
    "transpile": "babel . -d dist/. -s inline -i './**/*.test.js'",
    "build": "npm run clean && npm run copy && npm run transpile",
    "docs": "apidoc -i . -o docs && apidoc-markdown -p docs -o DOCS.md && opn docs/index.html"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime",
      "add-module-exports"
    ]
  },
  "ava": {
    "files": [
      "./**/*.test.js"
    ],
    "require": [
      "babel-register"
    ],
    "concurrency": 2,
    "timeout": "1m",
    "babel": "inherit"
  },
  "dependencies": {
    "apidoc": "^0.16.1",
    "apidoc-markdown": "^0.2.0",
    "ava": "^0.16.0",
    "babel-cli": "6.16.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "copy": "^0.3.0",
    "cross-env": "^3.0.0",
    "del-cli": "^0.2.0",
    "dotenv-safe": "^2.3.1",
    "eslint": "^3.3.1",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "mockgoose": "^6.0.8",
    "nock": "^8.0.0",
    "nodemon": "^1.10.2",
    "nyc": "^8.1.0",
    "opn-cli": "^3.1.0",
    "sinon": "^1.17.5",
    "supertest": "^2.0.0",
    "supertest-as-promised": "^4.0.0",
    "bluebird": "^3.4.1",
    "body-parser": "1.15.2",
    "cookie-parser": "1.4.3",
    "dotenv": "2.0.0",
    "ejs": "2.5.2",
    "express": "^4.14.0",
    "helmet": "^2.1.2",
    "jade": "1.11.0",
    "lodash": "^4.15.0",
    "mongoose": "^4.5.9",
    "morgan": "^1.7.0"
  },
  "devDependencies": {
    "babel-runtime": "^6.11.6",
    "bluebird": "^3.4.0",
    "body-parser": "^1.15.1",
    "bodymen": "^1.0.1",
    "compression": "^1.6.2",
    "cors": "^2.7.1",
    "express": "^4.13.4",
    "lodash": "^4.13.1",
    "mongoose": "^4.4.19",
    "mongoose-create-unique": "^0.4.1",
    "mongoose-keywords": "^0.3.1",
    "morgan": "^1.7.0",
    "querymen": "^2.0.0",
    "request": "^2.74.0",
    "request-promise": "^4.1.1",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "feathers-cli": "1.2.7",
    "jshint": "2.9.3",
    "mocha": "3.1.0",
    "nodemon": "^1.9.2",
    "pm2": "2.0.18"
  }
}
